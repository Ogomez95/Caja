<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="beans.Detalle" table="DETALLE">
		<id name="idDet" type="java.lang.Integer">
			<column name="idDet"></column>
			<generator class="increment"/>
		</id>
		<property name="serie" type="java.lang.String">
			<column name="serie"></column>
		</property>
                <property name="codigo" type="java.lang.String">
			<column name="codigo"></column>
		</property>
		<property name="nombre" type="java.lang.String">
			<column name="nombre"></column>
		</property>
                <property name="precio" type="java.lang.Double">
			<column name="precio"></column>
		</property>
	</class>
</hibernate-mapping>
