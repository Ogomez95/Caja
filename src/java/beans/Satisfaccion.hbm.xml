<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="beans.Satisfaccion" table="Satisfaccion">
		<id name="idSat" type="java.lang.Integer">
			<column name="idSat"></column>
			<generator class="increment"/>
		</id>
		<property name="serie" type="java.lang.String">
			<column name="serie"></column>
		</property>
                <property name="valor" type="java.lang.Integer">
			<column name="valor"></column>
		</property>
	</class>
</hibernate-mapping>
